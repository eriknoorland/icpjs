{
  "name": "@eriknoorland/icpjs",
  "version": "0.2.2",
  "description": "A Javascript library written in Typescript to perform the iterative closest point (ICP) algorithm on two 2D point clouds.",
  "files": [
    "dist"
  ],
  "main": "./dist/icpjs.umd.cjs",
  "module": "./dist/icpjs.js",
  "types": "./dist/icpjs.d.ts",
  "exports": {
    ".": {
      "import": "./dist/icpjs.js",
      "require": "./dist/icpjs.umd.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest"
  },
  "keywords": [
    "icp",
    "algorithm",
    "robotics",
    "javascript",
    "typescript"
  ],
  "author": {
    "name": "Erik Noorland",
    "url": "https://github.com/eriknoorland"
  },
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "rollup": "^4.9.0",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
